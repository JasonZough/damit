{
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier",
        "tslint-react"
    ],
    "rules": {
        "no-submodule-imports": [false],
        "object-literal-sort-keys": false,
        "no-console": false,
        "jsx-no-multiline-js": false,
        "jsx-self-close": false
    }
}